    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "yarn lint",
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "./cz-adapter.cjs"
    }
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "prettier": "3.2.5"
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
